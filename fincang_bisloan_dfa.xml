            <gurantors/>
            <gurantor1_name/>
            <gurantor1_occup/>
            <gurantor1_known_client/>
            <gurantor1_relat_client/>
            <gurantor1_photo/>
            <commit_gurantor1/>
            <gurantor2_name/>
            <gurantor2_occup/>
            <gurantor2_known_client/>
            <gurantor2_relat_client/>
            <gurantor2_photo/>
            <commit_gurantor2/>
            <gender_list/>
          </begin_group>
          <begin_group1>
            <date_time_visit/>
            <client_geopoint/>
            <client_bistype/>
            <startup_amount/>
            <presentworth_biz/>
            <bankcash_bal/>
            <acct_receivables/>
            <acct_payable/>
            <presentstock_value/>
            <workinprog/>
            <inventory/>
          </begin_group1>
          <clients_branch/>
          <clients_district/>
          <settlement_type/>
          <begin_group2>
            <dwellingdetails/>
            <household_size/>
            <flooringmat_list/>
            <flooringmat_others_specified/>
            <roofingmat_list/>
            <roofingmat_others_specified/>
          </begin_group2>
          <clientsex/>
          <loan_interest_rate/>
          <client_dateofbirth/>
          <client_home_address/>
          <interviewlocation_list/>
          <others/>
          <interviewlocation_address/>
          <dateofinterview/>
          <product_types/>
          <otherpro/>
          <amountvill_loan/>
          <channel_villaLoan/>
          <distance_to_branch_location1/>
          <duration_of_interview1/>
          <interest_rate_onloan/>
          <clientage/>
          <client_photo1/>
          <clients_mobilenumber/>
          <record_clientresponse_groupmeeting/>
          <client_dwelling_exteriorwall_materialused/>
          <client_geopoint/>
          <client_missedworkday/>
          <number_missedworkdays/>
          <client_healthstatus_now/>
          <endnote/>
          <begin_group3>
            <householdmembers_list/>
            <number_mealsday_hhm/>
            <householdhead_list/>
          </begin_group3>
          <changeshh1/>
          <specific_changes1/>
          <specific_changes2/>
          <specific_changes3/>
          <specific_changes4/>
          <specific_changes5/>
          <specific_changes6/>
          <assessment/>
          <signature/>
          <meta>
            <instanceID/>
          </meta>
        </fincang_bisloan_dfa>
      </instance>
