      <bind nodeset="/fincang_bisloan_dfa/proof_bizownership" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/application" readonly="true()" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/account_statement_list" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/client_photo" type="binary"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantors" readonly="true()" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor1_name" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor1_occup" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor1_known_client" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor1_relat_client" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor1_photo" type="binary"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/commit_gurantor1" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor2_name" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor2_occup" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor2_known_client" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor2_relat_client" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gurantor2_photo" type="binary"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/commit_gurantor2" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group/gender_list" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/date_time_visit" type="dateTime"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/client_geopoint" type="geopoint"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/client_bistype" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/startup_amount" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/presentworth_biz" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/bankcash_bal" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/acct_receivables" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/acct_payable" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/presentstock_value" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/workinprog" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group1/inventory" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/clients_branch" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/clients_district" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/settlement_type" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group2/dwellingdetails" readonly="true()" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group2/household_size" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group2/flooringmat_list" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group2/flooringmat_others_specified" relevant=" /fincang_bisloan_dfa/begin_group2/flooringmat_list  = 'floor_otherspecify'" required="true()" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group2/roofingmat_list" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group2/roofingmat_others_specified" relevant=" /fincang_bisloan_dfa/begin_group2/roofingmat_list  = 'roof_otherspecify'" required="true()" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/clientsex" required="true()" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/loan_interest_rate" required="true()" type="decimal"/>
      <bind nodeset="/fincang_bisloan_dfa/client_dateofbirth" type="date"/>
      <bind nodeset="/fincang_bisloan_dfa/client_home_address" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/interviewlocation_list" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/others" relevant=" /fincang_bisloan_dfa/interviewlocation_list  = 'others_specify'" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/interviewlocation_address" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/dateofinterview" type="date"/>
      <bind nodeset="/fincang_bisloan_dfa/product_types" type="select"/>
      <bind nodeset="/fincang_bisloan_dfa/otherpro" relevant=" /fincang_bisloan_dfa/product_types  = 'pro_others_specify'" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/amountvill_loan" relevant="selected( /fincang_bisloan_dfa/product_types , pr_villagebank_loan)" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/channel_villaLoan" relevant="selected( /fincang_bisloan_dfa/product_types , pr_villagebank_loan)" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/distance_to_branch_location1" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/duration_of_interview1" type="int"/>
      <bind constraint=".&lt;=2 or .=999" nodeset="/fincang_bisloan_dfa/interest_rate_onloan" type="decimal"/>
      <bind constraint=". &gt;=30 and . &lt;=50" jr:constraintMsg="client's age is not allowed" nodeset="/fincang_bisloan_dfa/clientage" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/client_photo1" type="binary"/>
      <bind nodeset="/fincang_bisloan_dfa/clients_mobilenumber" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/record_clientresponse_groupmeeting" type="binary"/>
      <bind nodeset="/fincang_bisloan_dfa/client_dwelling_exteriorwall_materialused" type="binary"/>
      <bind nodeset="/fincang_bisloan_dfa/client_geopoint" type="geopoint"/>
      <bind nodeset="/fincang_bisloan_dfa/client_missedworkday" type="select1"/>
      <bind constraint=". &lt;=30" jr:constraintMsg="Missed workdays more than 30" nodeset="/fincang_bisloan_dfa/number_missedworkdays" required="true()" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/client_healthstatus_now" required="true()" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/endnote" readonly="true()" type="string"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group3/householdmembers_list" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group3/number_mealsday_hhm" type="int"/>
      <bind nodeset="/fincang_bisloan_dfa/begin_group3/householdhead_list" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/changeshh1" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/specific_changes1" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/specific_changes2" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/specific_changes3" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/specific_changes4" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/specific_changes5" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/specific_changes6" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/assessment" type="select1"/>
      <bind nodeset="/fincang_bisloan_dfa/signature" type="binary"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/fincang_bisloan_dfa/meta/instanceID" readonly="true()" type="string"/>
    </model>
